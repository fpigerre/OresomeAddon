{
    "manifest_version": 2,

    "name": "Oresome Addon",
    "description": "This extension integrates OresomeCraft features in Google Chrome!",
    "version": "1.0",

    "permissions": [
        "https://oresomecraft.com/",
        "notifications"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}